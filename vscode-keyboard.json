// 将键绑定放在此文件中以覆盖默认值auto[]
[
  {
      "key": "ctrl+shift+r",
      "command": "workbench.action.tasks.runTask"
  },
  {
      "key": "alt+j",
      "command": "selectNextSuggestion",
      "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
      "key": "alt+h",
      "command": "workbench.action.focusLeftGroup"
  },
  {
      "key": "alt+l",
      "command": "workbench.action.focusRightGroup"
  },
  {
      "key": "alt+b",
      "command": "workbench.files.action.focusFilesExplorer"
  },
  {
      "key": "alt+k",
      "command": "selectPrevSuggestion",
      "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
      "key": "ctrl+shift+j",
      "command": "-workbench.action.search.toggleQueryDetails",
      "when": "inSearchEditor || searchViewletFocus"
  },
  {
      "key": "ctrl+shift+j",
      "command": "editor.action.triggerParameterHints",
      "when": "editorHasSignatureHelpProvider && editorTextFocus"
  },
  {
      "key": "ctrl+k ctrl+left",
      "command": "-workbench.action.focusLeftGroup"
  },
  {
      "key": "ctrl+k ctrl+right",
      "command": "-workbench.action.focusRightGroup"
  },
  {
      "key": "ctrl+alt+,",
      "command": "workbench.action.moveEditorToPreviousGroup"
  },
  {
      "key": "ctrl+k left",
      "command": "-workbench.action.moveActiveEditorGroupLeft"
  },
  {
      "key": "ctrl+alt+.",
      "command": "workbench.action.moveEditorToNextGroup"
  },
  {
      "key": "ctrl+k right",
      "command": "-workbench.action.moveActiveEditorGroupRight"
  },
  {
      "key": "ctrl+k l",
      "command": "workbench.action.moveEditorToNextGroup"
  },
  {
      "key": "ctrl+alt+right",
      "command": "-workbench.action.moveEditorToNextGroup"
  },
  {
      "key": "ctrl+k h",
      "command": "workbench.action.moveEditorToPreviousGroup"
  },
  {
      "key": "ctrl+alt+left",
      "command": "-workbench.action.moveEditorToPreviousGroup"
  },
  {
      "key": "alt+h",
      "command": "-testing.toggleTestingPeekHistory",
      "when": "testing.isPeekVisible"
  },
  {
      "key": "ctrl+d",
      "command": "-list.focusPageDown",
      "when": "listFocus && !inputFocus"
  },
  {
      "key": "ctrl+d",
      "command": "-editor.action.addSelectionToNextFindMatch",
      "when": "editorFocus"
  },
  {
      "key": "ctrl+d",
      "command": "-extension.vim_ctrl+d",
      "when": "editorTextFocus && vim.active && vim.use<C-d> && !inDebugRepl"
  },
  {
      "key": "ctrl+alt+d",
      "command": "workbench.debug.action.toggleRepl",
      "when": "workbench.panel.repl.view.active"
  },
  // {
  //     "key": "ctrl+d",
  //     "command": "workbench.debug.action.toggleRepl",
  //     "when": "workbench.panel.repl.view.active"
  // },
  {
      "key": "ctrl+shift+y",
      "command": "-workbench.debug.action.toggleRepl",
      "when": "workbench.panel.repl.view.active"
  },
  {
      "key": "alt+h",
      "command": "workbench.action.focusLeftGroup"
  },
  {
      "key": "ctrl+shift+y",
      "command": "workbench.debug.action.toggleRepl",
      "when": "workbench.panel.repl.view.active"
  },
  {
      "key": "alt+k",
      "command": "workbench.action.terminal.focusPrevious",
      "when": "terminalFocus"
  },
  {
      "key": "alt+j",
      "command": "workbench.action.terminal.focusNext",
      "when": "terminalFocus"
  },
  // {
  //     "key": "ctrl+e",
  //     "command": "workbench.action.toggleSidebarVisibility"
  // },
  {
      "key": "ctrl+alt+e",
      "command": "workbench.files.action.focusFilesExplorer",
      "when": "editorTextFocus"
  },
  {
      "key": "ctrl+e",
      "command": "workbench.files.action.focusFilesExplorer",
      "when": "editorTextFocus"
  },
  {
      "key": "a",
      "command": "explorer.newFile",
      "when": "filesExplorerFocus && !inputFocus"
  },
  {
      "key": "r",
      "command": "renameFile",
      "when": "filesExplorerFocus && !inputFocus"
  },
  {
      "key": "c",
      "command": "filesExplorer.copy",
      "when": "filesExplorerFocus && !inputFocus"
  },
  {
      "key": "p",
      "command": "filesExplorer.paste",
      "when": "filesExplorerFocus && !inputFocus"
  },
  {
      "key": "d",
      "command": "deleteFile",
      "when": "filesExplorerFocus && !inputFocus"
  },
  {
      "key": "ctrl+m",
      "command": "workbench.action.maximizeEditor"
  },
  {
      "key": "ctrl+alt+m",
      "command": "workbench.action.toggleMaximizedPanel"
  },
  {
      "key": "ctrl+i",
      "command": "-editor.action.triggerSuggest",
      "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly"
  },
  {
      "key": "ctrl+alt+y",
      "command": "workbench.debug.action.toggleRepl",
      "when": "workbench.panel.repl.view.active"
  },
  {
      "key": "ctrl+d",
      "command": "workbench.action.terminal.toggleTerminal",
      "when": "terminal.active"
  },
  {
      "key": "ctrl+alt+i",
      "command": "workbench.action.terminal.toggleTerminal",
      "when": "terminal.active"
  },
  {
      "key": "ctrl+n",
      "command": "workbench.action.terminal.new",
      "when": "terminalFocus && terminalProcessSupported || terminalWebExtensionContributedProfile"
  },
  {
      "key": "ctrl+j",
      "command": "workbench.action.terminal.scrollDown",
      "when": "terminalFocus && !editorFocus"
  },
  {
      "key": "ctrl+k",
      "command": "workbench.action.terminal.scrollUp",
      "when": "terminalFocus && !editorFocus"
  },
  {
      "key": "ctrl+alt+j",
      "command": "editor.action.nextMatchFindAction",
      "when": "editorFocus"
  },
  {
      "key": "ctrl+alt+k",
      "command": "editor.action.previousMatchFindAction",
      "when": "editorFocus"
  },
  {
      "key": "ctrl+alt+j",
      "command": "workbench.action.terminal.findNext",
      "when": "terminalFocus"
  },
  {
      "key": "ctrl+alt+k",
      "command": "workbench.action.terminal.findPrevious",
      "when": "terminalFocus"
  },
  {
      "key": "ctrl+e",
      "command": "-workbench.action.quickOpen"
  },
  {
      "key": "alt+n",
      "command": "workbench.action.debug.callStackDown"
  },
  {
      "key": "alt+shift+n",
      "command": "workbench.action.debug.callStackUp"
  },
  {
      "key": "ctrl+alt+-",
      "command": "workbench.action.decreaseViewWidth"
  },
  {
      "key": "ctrl+alt+=",
      "command": "workbench.action.increaseViewWidth"
  },
  {
      "key": "ctrl+alt+0",
      "command": "workbench.action.evenEditorWidths"
  },
  {
      "key": "ctrl+]",
      "command": "-editor.action.indentLines",
      "when": "editorTextFocus && !editorReadonly"
  },
  {
      "key": "ctrl+[",
      "command": "-editor.action.outdentLines",
      "when": "editorTextFocus && !editorReadonly"
  },
  {
      "key": "ctrl+alt+t n",
      "command": "notes.focus",
  },
  {
      "key": "ctrl+alt+c n",
      "command": "Notes.newNote",
  },
  {
      "key": "ctrl+alt+n",
      "command": "search.action.focusNextSearchResult"
  },
  {
      "key": "ctrl+alt+p",
      "command": "search.action.focusPreviousSearchResult"
  },
  {
      "key": "ctrl+alt+t q",
      "command": "search.action.focusSearchList"
  },
  {
      "key": "ctrl+alt+o",
      "command": "editor.action.triggerParameterHints"
  },
  {
      "key": "F4",
      "command": "workbench.action.debug.stop"
  },
  {
      "key": "ctrl+end",
      "command": "-interactive.scrollToBottom",
      "when": "resourceScheme == 'vscode-interactive'"
  },
  {
      "key": "ctrl+home",
      "command": "-interactive.scrollToTop",
      "when": "resourceScheme == 'vscode-interactive'"
  },
  {
      "key": "ctrl+o",
      "command": "-workbench.action.files.openFile",
      "when": "true"
  },
  {
      "key": "ctrl+o",
      "command": "-workbench.action.files.openFileFolder",
      "when": "isMacNative && openFolderWorkspaceSupport"
  },
  {
      "key": "ctrl+o",
      "command": "-workbench.action.files.openFolderViaWorkspace",
      "when": "!openFolderWorkspaceSupport && workbenchState == 'workspace'"
  },
  {
      "key": "ctrl+i",
      "command": "workbench.action.navigateForward",
      "when": "canNavigateForward"
  },
  {
      "key": "ctrl+o",
      "command": "workbench.action.navigateBack",
      "when": "canNavigateBack"
  },
  {
      "key": "ctrl+alt+r",
      "command": "-workbench.action.terminal.runRecentCommand",
      "when": "terminalFocus && terminalHasBeenCreated && !accessibilityModeEnabled || terminalFocus && terminalProcessSupported && !accessibilityModeEnabled"
  },
  {
      "key": "ctrl+alt+r",
      "command": "-workbench.action.terminal.sendSequence",
      "when": "accessibilityModeEnabled && terminalFocus"
  },
  {
      "key": "ctrl+alt+r",
      "command": "code-runner.run"
  },
  {
      "key": "ctrl+alt+,",
      "command": "editor.action.scrollUpHover",
  },
  {
      "key": "ctrl+alt+.",
      "command": "editor.action.scrollDownHover",
  },
  {
      "key": "ctrl+j",
      "command": "togglePeekWidgetFocus",
      "when": "inReferenceSearchEditor || referenceSearchVisible"
  },
  {
      "key": "ctrl+k ctrl+k",
      "command": "editor.action.showHover",
      "when": "editorTextFocus"
  },
  {
      "key": "ctrl+k ctrl+k",
      "command": "editor.debug.action.showDebugHover",
      "when": "editorTextFocus && inDebugMode"
  },
  {
      "key": "ctrl+k ctrl+k",
      "command": "workbench.action.terminal.focusHover",
      "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalIsOpen || terminalFocus && terminalProcessSupported || terminalHasBeenCreated && terminalTabsFocus || terminalIsOpen && terminalTabsFocus || terminalProcessSupported && terminalTabsFocus"
  },
  {
      "key": "ctrl+k ctrl+k",
      "command": "workbench.action.showTreeHover",
      "when": "customTreeView && listFocus && !inputFocus"
  },
  // {
  //     "key": "ctrl+.",
  //     "command": "editor.action.showHover",
  //     "when": "editorTextFocus"
  // }
]